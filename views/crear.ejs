<%- include ('./template/cabecera', {tituloa: 'Crear Cliente' }); %>
<h1 align = 'center' class="text-light bg-info">Crear cliente</h1>

<meta name="description" content="">
<meta name="viewport" content="width=device-width, initial-scale=1">

<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
<link rel="stylesheet" href="../css/style.css">

<div class="container">
    <form action="/clientes" method="post">
        <input type="text"
        placeholder ="Agregar nombre del cliente"
        class="form-control my-2"
        name="nombre">

        <input type="text"
        placeholder ="Agregar apellido del cliente"
        class="form-control my-2"
        name="apellido">

        <input type="text"
        placeholder ="Agregar celular del cliente"
        class="form-control my-2"
        name="cel">

        <input type="text"
        placeholder ="Agregar cedula del cliente"
        class="form-control my-2"
        name="cedula">

        <input type="text"
        placeholder ="Agregar direccion del cliente"
        class="form-control my-2"
        name="direccion">

        <input type="text"
        placeholder ="Agregar Email del cliente"
        class="form-control my-2"
        name="email">

        <button
        class = "btn btn-success btn-block"
        type="submit">
            agregar cliente
            </button>
            
            
<%- include('./template/footer'); %>